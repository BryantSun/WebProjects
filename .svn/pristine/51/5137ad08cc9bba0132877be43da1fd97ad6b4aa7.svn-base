    @Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("Mailbox [id=").append(id).append(", name=").append(name).append(", telephone=")
				.append(telephone).append(", email=").append(email).append(", credentialstype=").append(credentialstype)
				.append(", credentialscode=").append(credentialscode).append(", contactaddress=").append(contactaddress)
				.append(", messagetitle=").append(messagetitle).append(", content=").append(content)
				.append(", attachmentpath=").append(attachmentpath).append(", isopen=").append(isopen)
				.append(", lettertype=").append(lettertype).append(", sex=").append(sex).append("]");
		return builder.toString();
	}


	public Integer getSex() {
		return sex;
	}


	public void setSex(Integer sex) {
		this.sex = sex;
	}


	public Integer getId() {
        return id;
    }

    
    public void setId(Integer id) {
