package com.information.platform.bean;

public class Mailbox {
    
    private Integer id;

   //姓名
    private String name;

   //联系电话
    private String telephone;

   //邮箱地址
    private String email;

    //证件类型
    private Integer credentialstype;

    //证件编码
    private String credentialscode;

    //联系地址
